{
  "Logging": {
    "PathFormat": "Logs/{Date}.log",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Information"
    }
  },
  "Debugging": {
    "UseLoggingBehavior": false,
    "UseHttpLoggingMiddleware": false
  }
}
